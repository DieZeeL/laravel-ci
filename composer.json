{
    "name": "diezeel/laravel-ci",
    "description": "Laravel CI implementation",
    "keywords": [
        "laravel",
        "CI",
        "CodeIgniter"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Kornienko",
            "email": "diez.eel@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "laravel/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "diezeel\\CI\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "diezeel\\CI\\CIServiceProvider"
            ]
        }
    }
}
